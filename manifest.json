{
  "name": "Teamwork.com Quick View",
  "description": "See your open tasks from Teamwork.com every time you open new tab",
  "version": "2.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "twtasks.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.teamwork.com https://*.eu.teamwork.com; object-src 'self'",
  "permissions": [
      "storage",
      "https://*.teamwork.com/*",
      "https://*.eu.teamwork.com/*"
  ], 
  "icons": {
		"16": "/img/icon16.png",
		"48": "/img/icon48.png",
		"128": "/img/icon128.png" 
  },
  "manifest_version": 2
}